<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_settings_graph"
    app:startDestination="@id/settingsFragment">

    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.example.makemon.ui.settings.SettingsFragment"
        android:label="SettingsFragment"
        tools:layout="@layout/fragment_settings">
        <action
            android:id="@+id/action_settingsFragment_to_testFragment"
            app:destination="@id/testFragment" />
        <action
            android:id="@+id/action_settingsFragment_to_testTwoFragment"
            app:destination="@id/testTwoFragment" />
    </fragment>
    <fragment
        android:id="@+id/testFragment"
        android:name="com.example.makemon.ui.test.TestFragment"
        android:label="TestFragment" />
    <fragment
        android:id="@+id/testTwoFragment"
        android:name="com.example.makemon.ui.test.TestTwoFragment"
        android:label="TestTwoFragment" />
</navigation>